{
  "name": "server",
  "version": "1.0.0",
  "main": "./build/src/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "develop": "ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "eslint \"./src/**\"",
    "watch": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node build/src/index.js",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"src/tests/**/*.test.ts\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "_moduleAlisases": {
    "@constants": "build/src/constants",
    "@core": "build/src/core",
    "@domainTypes": "build/src/types",
    "@middleware": "build/src/middleware",
    "@routes": "build/src/routes",
    "@services": "build/src/services",
    "@src": "build/src",
    "@utils": "build/src/utils"
  }
}
